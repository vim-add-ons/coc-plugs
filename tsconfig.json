{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "es2018","es6"],
    "module": "commonjs",
    "declaration": false,
    "sourceMap": true,
    "outDir": "lib",
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "jsx": "preserve",
    "pretty": true,
    "checkJs": true,
    "allowJs": true
  },
  "include": ["src"]
}
